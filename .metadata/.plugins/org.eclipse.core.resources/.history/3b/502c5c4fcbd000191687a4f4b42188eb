package question4;

public class Money {
	float paisa;
	float rupee;
	Money(float rupee,float paisa){
		this.rupee=rupee;
		this.paisa=paisa;
	}
	float addition(float paisa) {
		
		float sum=rupee+paisa;
		return sum;
	}
	float subtraction(float paisa) {
		float sub=rupee-paisa;
		return sub;
	}

}
